<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="100dip" />
            <stroke android:width="9dip" android:color="#000000" />
            <gradient android:angle="-90" android:startColor="#C0C404" android:endColor="#ebe715"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="100dip" />
            <stroke android:width="9dip" android:color="#000000" />
            <solid android:color="#ebe715"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="100dip" />
            <stroke android:width="9dip" android:color="#000000" />
            <gradient android:angle="-90" android:startColor="#ebe715" android:endColor="#ebe715" />
        </shape>
    </item>
</selector>